{
  "name": "ct2-suppliers-service",
  "version": "0.4.1",
  "private": true,
  "type": "module",
  "main": "server.mjs",
  "scripts": {
    "start": "node server.mjs",
    "dev": "node --watch server.mjs",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --runInBand"
  },
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "fastify": "^4.29.1",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "jest": "^29.7.0"
  },
  "jest": {
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/__tests__"
    ],
    "moduleFileExtensions": [
      "js",
      "mjs",
      "json"
    ],
    "extensionsToTreatAsEsm": [
      ".mjs"
    ],
    "verbose": true
  }
}
